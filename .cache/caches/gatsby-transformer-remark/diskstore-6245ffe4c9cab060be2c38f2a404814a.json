osh19/"},"children":[{"type":"text","value":"Instagram account","position":{"start":{"line":52,"column":200,"offset":5037},"end":{"line":52,"column":217,"offset":5054}}}],"position":{"start":{"line":52,"column":199,"offset":5036},"end":{"line":52,"column":256,"offset":5093}}},{"type":"text","value":" and you can download them on my ","position":{"start":{"line":52,"column":256,"offset":5093},"end":{"line":52,"column":289,"offset":5126}}},{"type":"element","tagName":"a","properties":{"href":"https://unsplash.com/@smakosh"},"children":[{"type":"text","value":"Unsplash account","position":{"start":{"line":52,"column":290,"offset":5127},"end":{"line":52,"column":306,"offset":5143}}}],"position":{"start":{"line":52,"column":289,"offset":5126},"end":{"line":52,"column":338,"offset":5175}}},{"type":"text","value":" for free.","position":{"start":{"line":52,"column":338,"offset":5175},"end":{"line":52,"column":348,"offset":5185}}}],"position":{"start":{"line":52,"column":1,"offset":4838},"end":{"line":52,"column":348,"offset":5185}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"I personally prefer natural & traditional spots but I made sure to cover modern ones as well for the sake of my family, friends and followers on social media who might prefer to visit them in the future.","position":{"start":{"line":54,"column":1,"offset":5187},"end":{"line":54,"column":204,"offset":5390}}}],"position":{"start":{"line":54,"column":1,"offset":5187},"end":{"line":54,"column":204,"offset":5390}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{"id":"weather","style":"position:relative;"},"children":[{"type":"element","tagName":"a","properties":{"href":"#weather","aria-label":"weather permalink","class":"anchor before"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=