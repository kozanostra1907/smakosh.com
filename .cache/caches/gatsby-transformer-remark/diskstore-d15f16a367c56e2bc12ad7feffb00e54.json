.","position":{"start":{"line":48,"column":3,"offset":2746},"end":{"line":49,"column":555,"offset":3319}}}],"position":{"start":{"line":48,"column":3,"offset":2746},"end":{"line":49,"column":555,"offset":3319}}},{"type":"text","value":"\n"},{"type":"element","tagName":"blockquote","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"They do refund your buses tickets if you came by the bus, in my case I booked a Grab drive which I don’t recommend except if you want to save time.","position":{"start":{"line":51,"column":5,"offset":3325},"end":{"line":51,"column":152,"offset":3472}}}],"position":{"start":{"line":51,"column":5,"offset":3325},"end":{"line":51,"column":152,"offset":3472}}},{"type":"text","value":"\n"}],"position":{"start":{"line":51,"column":3,"offset":3323},"end":{"line":51,"column":152,"offset":3472}}},{"type":"text","value":"\n"}],"position":{"start":{"line":48,"column":1,"offset":2744},"end":{"line":51,"column":152,"offset":3472}}},{"type":"text","value":"\n"}],"position":{"start":{"line":48,"column":1,"offset":2744},"end":{"line":51,"column":152,"offset":3472}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"gatsby-resp-iframe-wrapper\" style=\"padding-bottom: 56.25%; position: relative; height: 0; overflow: hidden; margin-bottom: 1.0725rem\" > <div class=\"responsiveVideo\"> <iframe src=\"https://www.youtube.com/embed/BHmS-h8Jt9w\" frameborder=\"0\" allow=\"encrypted-media\" allowfullscreen style=\" position: absolute; top: 0; left: 0; width: 100%; height: 100%; \"></iframe> </div> </div>","position":{"start":{"line":53,"column":1,"offset":3474},"end":{"line":55,"column":7,"offset":3661}}},{"type":"text","value":"\n"},{"type":"element","tagName":"ul","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"ch